{
    "name": "gourmet/common-mark"
    , "description": "Adds CommonMark support to CakePHP 3"
    , "type": "cakephp-plugin"
    , "keywords": [
        "cakephp"
        , "gourmet"
        , "commonmark"
        , "markdown"
    ]
    , "homepage": "https://github.com/gourmet/common-mark"
    , "license": "MIT"
    , "authors": [
        {
            "name": "Jad Bitar"
            , "homepage": "http://jadb.io"
            , "role": "Author"
        }
        , {
            "name": "Others"
            , "homepage": "https://github.com/gourmet/common-mark/graphs/contributors"
        }
    ]
    , "support": {
        "issues": "https://github.com/gourmet/common-mark/issues"
        , "source": "https://github.com/gourmet/common-mark"
    }
    , "require": {
        "cakephp/cakephp": "3.0.*-dev"
        , "cakephp/plugin-installer": "*"
        , "league/commonmark": "~0.5"
    }
    , "require-dev": {
        "phpunit/phpunit": "4.1.*"
    }
    , "autoload": {
        "psr-4": {
            "Gourmet\\CommonMark\\": "src"
        }
    }
    , "autoload-dev": {
        "psr-4": {
            "Gourmet\\CommonMark\\Test\\": "tests"
        }
    }
}
